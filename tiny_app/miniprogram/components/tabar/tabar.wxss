/* components/tabar.wxss */
.tabar{
  position: fixed;
  bottom:0;
  width: 100%;
  height: 104rpx;
  display: flex;
  background: #f581ce;
}
.tabar .tabar-item{
  flex: 1;
  align-self: center;
  text-align: center;
}
.tabar .tabar-item .icon{
  display: block;
  margin: 0 auto;
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 4rpx;
}
.tabar .tabar-item text{
  display: block;
  font-size:30rpx;
  line-height: 1;
  color: #333;
}